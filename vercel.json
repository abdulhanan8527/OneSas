{
  "version": 2,
  "builds": [
    {
      "src": "OneSas/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "maxLambdaSize": "15mb",
        "installCommand": "pip install -r requirements.txt && python manage.py collectstatic --noinput"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles_build"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "https://nyvzcrdoscvrsvivaitg.supabase.co/storage/v1/object/public/one-sas/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "OneSas/wsgi.py"
    }
  ]
}